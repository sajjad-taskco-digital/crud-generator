{
    "name": "taskcodigital/crud-generator",
    "description": "Laravel API CRUD Generator Package - Generate complete API CRUD operations with controllers, services, requests, migrations, and seeders",
    "keywords": ["laravel", "crud", "api", "generator", "taskcodigital"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "TaskcoDigital",
            "email": "info@taskcodigital.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "laravel/framework": "^10.0|^11.0|^12.0"
    },
    "autoload": {
        "psr-4": {
            "TaskcoDigital\\CrudGenerator\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "TaskcoDigital\\CrudGenerator\\CrudGeneratorServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable"
}
